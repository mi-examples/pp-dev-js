    "http-proxy-middleware": "^2.0.6",
    "jsdom": "^22.0.0",
    "picocolors": "^1.0.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vite-plugin-zip-pack": "^1.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1"
  },
  "files": [
    "bin",
    "dist",
    "package.json",
    "README.md",
