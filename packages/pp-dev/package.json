{
  "name": "@metricinsights/pp-dev",
  "type": "module",
  "version": "0.0.1",
  "description": "Portal Page dev build tool",
  "bin": {
    "pp-dev": "bin/pp-dev.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "cd test && npm run build"
